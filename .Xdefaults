! --[ Font Rendering ]-------------------------------------
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! --[ Rodent ]----------------------------------------------
Xcursor.theme: DMZ-White
Xcursor.size:  24

! --[ URxvt - General ]-------------------------------------
URxvt.depth: 32
URxvt.scrollBar:false
URxvt.saveLines:5000
URxvt.cursorColor:#83C048
URxvt.highlightColor:#444444
URxvt.font: xft:DroidSansMono:pixelsize=12
URxvt.loginshell: true
URxvt.internalborder: 0
URxvt.underlinecolor: #999999
URxvt.cutchars: "()*,<>[]{}|'
URxvt.print-pipe: cat >/tmp/xxx
URxvt.visualBell: false

! --[ URxvt - URL Handling ]--------------------------------
URxvt.keysym.M-u: perl:mark-yank-urls:activate_mark_mode
URxvt.perl-ext: selection,mark-yank-urls
URxvt.urlLauncher: uzbl-browser
URxvt.underlineURLs: true

! --[ URxvt - colors ]--------------------------------------
URxvt.background: rgba:0000/0000/0000/bbbb
URxvt.foreground:#eeeeee

